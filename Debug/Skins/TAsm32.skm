[SKINFOS]
Title=TAsm32

[EXTENSION]
Ext=asm

[EXTRAS]
TrapStdErr=0
ObjLstPrefix=

[PASCALNAME]
WINEXE=
DOSEXE=
WINDLL=
WINLIB=
WINSYS=
DOSCOM=

[INCLUDE]
IncType=include %0

[INCLUDELIB]
EarlyInclude=0
PreInc=
InInc=
PostInc=
IncType=includelib %0

[CONSTANTS]
Const=%0 equ %1

[RESP]
RespSep={SPC}
RespDirect=
Resp000=""%A.obj""
Resp001={OBJS}

[LIBSPEC]
ForeChar=+
BackChar=
RetrieveChar=

[RES]
AddResLanguage=1
ExtendedRes=1
AddVInfo=1
StopOnError=1
UseRawComma=0
Prog000=[BINDIR]\brc32.exe
Args000=-32 -r -fo"%A.res" "%A.trc"

[COMPILER]
StopOnError=1
Prog000=[BINDIR]\tasm32.exe
Args000=/t /m5 %B %C %D %E %H "%9","%A.obj"

[LINKER]
HeadLib=0
PrefixLib=
StopOnError=1
DllEntryPoint=
DOSCom000=del
ProgWINEXE000=[BINDIR]\tlink32.exe
ArgsWINEXE000=/Tpe /x /aa %F %G %I @"%A.ode","%A.exe",,,,"%A.res"
ProgDOSEXE000=[BINDIR]\tlink32.exe
ArgsDOSEXE000=/Tpe /x /aa %F %G %I @"%A.ode","%A.exe",,,,"%A.res"
ProgWINDLL000=[BINDIR]\tlink32.exe
ArgsWINDLL000=/Tpd /x /aa %F %G %I @"%A.ode","%A.dll",,,"%A.def","%A.res"
ProgWINLIB000=[BINDIR]\tlib.exe
ArgsWINLIB000=""%A.lib" @"%A.ode""

[TEMPHEADER]
Opt000=COMBO000
Opt001=COMBO001
Opt002=COMBO002
Opt003=CHECK003
